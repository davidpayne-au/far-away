{
  "name": "far-away",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "prebuild": "node scripts/generate-version-file.mjs",
    "build": "npm run prebuild && tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "NODE_OPTIONS='--no-deprecation' vitest run --exclude=tests/**/*.spec.ts",
    "validate": "npm run lint && npm run test",
    "test:e2e": "playwright test --config=playwright.config.ts"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.13",
    "framer-motion": "^12.23.18",
    "normalize.css": "^8.0.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.9.1",
    "tailwindcss": "^4.1.13"
  },
  "devDependencies": {
    "@axe-core/react": "^4.10.2",
    "@eslint/js": "^9.36.0",
    "@playwright/test": "^1.55.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@types/jest-axe": "^3.5.9",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.3",
    "axe-core": "^4.10.3",
    "dotenv": "^17.2.2",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.4.0",
    "jest-axe": "^10.0.0",
    "jsdom": "^27.0.0",
    "msw": "^2.11.3",
    "typescript": "~5.9.2",
    "typescript-eslint": "^8.44.0",
    "vitest": "^3.2.4",
    "vite": "^6.3.6"
  },
  "engines": {
    "node": "22.x"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
